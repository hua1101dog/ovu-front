	<!--新增修改弹出框  -->
	<div class="modal-header">
		<button type="button" class="close" ng-click="vm.cancel()">×</button>
		<h4 class="modal-title">审核不通过</h4>
    </div>
    <div class="modal-body container-fluid">
		<form name="form" novalidate class="form-horizontal">
			<div class="form-group">
				<label class="col-xs-3 control-label required">原因：</label>
				<div class="col-xs-8">
				     <textarea type="text" class="form-control" 
						required ng-model="vm.item.notPassReason" name="notPassReason" />
				</div>
			</div>
		</form>
	</div>
	<div class="modal-footer">
		<button class="btn btn-primary" ng-click="vm.save(form,vm.item)">确定</button>
		<button class="btn btn-warning" ng-click="vm.cancel()">取消</button>
	</div>

