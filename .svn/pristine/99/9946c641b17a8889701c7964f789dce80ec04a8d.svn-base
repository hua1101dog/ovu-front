<div class="modal-header">
	<button type="button" class="close" ng-click="cancel()">×</button>
	<h4 class="modal-title">{{item.id?'编辑':'新建'}}问卷</h4>
   </div>
   <div class="modal-body container-fluid form-horizontal ng-scope">
	<form name="form" novalidate="" class="ng-pristine ng-invalid ng-invalid-required">
		<div class="form-group">
			<label class="col-xs-2 control-label required text-left">标题：</label>
			<div class="col-xs-9">
				<input class="form-control" required  ng-model="item.title" name="title" type="text">
			</div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label required">问卷说明：</label>
			<div class="col-xs-9">
				<textarea class="form-control" rows="5" required ng-model="item.instruction" name="instruction"></textarea>
			</div>
		</div> 
		<div class="form-group">
	    	<label class="col-xs-2 control-label required">有效日期：</label>
	    	<div class="col-xs-9">
				<input type="datetime" class="form-control Wdate" ng-model="item.beginTime" id="naire_date1" onclick="javascript:WdatePicker({dateFmt:'yyyy-MM-dd',minDate:'%y-%M-%d',maxDate:'#F{$dp.$D(\'naire_date2\');}'})">
				 至
				<input type="datetime" class="form-control Wdate" ng-model="item.endTime" id="naire_date2" onclick="javascript:WdatePicker({dateFmt:'yyyy-MM-dd',minDate:'#F{$dp.$D(\'naire_date1\');}'})">
			</div>
		</div> 
	</form>
</div>
<div class="modal-footer">
	<button class="btn btn-primary" ng-click="saveQuestionnaire(form,item, $event)" >确定</button>
	<button class="btn btn-warning" ng-click="cancel()">取消</button>
</div>