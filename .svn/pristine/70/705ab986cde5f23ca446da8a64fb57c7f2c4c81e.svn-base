<div class="modal-header" style="border-bottom: none;">
    <button type="button" class="close" ng-click="cancel()">×</button>
    <h4 class="modal-title">租赁资源详情</h4>
</div>
<div class="modal-body container-fluid form-horizontal" style="padding-top: 0;margin-top: -10px;">
        <div class="row">
                <div class="x_panel" style="border: none;">
                    <ul class="nav nav-tabs bar_tabs" role="tablist">
                        <li role="presentation" class="active">
                            <a href="#baseDetail" id="tab1" role="tab" data-toggle="tab" aria-expanded="true" ng-click="getRentalHouseInfo()">基本信息</a>
                        </li>
                        <li role="presentation" class="">
                            <a href="#companyDetail" id="tab2" role="tab" id="profile-tab" data-toggle="tab" aria-expanded="false" ng-click="find(1)">合作信息</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <!-- 基本信息 -->
                        <div role="tabpanel" class="tab-pane fade active in" id="baseDetail">
                            <ul class="basaMsg_ul">
                                <li>
                                    <label>分期：</label>
                                    <span>{{baseMsg.stageName}}</span>
                                </li>
                                <li>
                                    <label>楼栋：</label>
                                    <span>{{baseMsg.buildName}}</span>
                                </li>
                                <li>
                                    <label>单元：</label>
                                    <span>{{baseMsg.unitNum}}</span>
                                </li>
                                <li>
                                    <label>楼层：</label>
                                    <span>{{baseMsg.groundNum}}</span>
                                </li>
                                <li>
                                    <label>房号：</label>
                                    <span>{{baseMsg.houseCode}}</span>
                                </li>
                                <li>
                                    <label>房屋名称：</label>
                                    <span>{{baseMsg.houseName}}</span>
                                </li>
                                <li>
                                    <label>房屋编码：</label>
                                    <span>{{baseMsg.houseNo}}</span>
                                </li>
                                <li>
                                    <label>面积（m
                                    <sup>2</sup>）：</label>
                                    <span>{{baseMsg.area}}</span>
                                </li>
                                <li>
                                    <label>资源状态：</label>
                                    <span>{{baseMsg.rentalStatus==1?"启用":"停用"}}</span>
                                </li>
                                <li>
                                    <label>租赁状态：</label>
                                    <span>{{baseMsg.spaceStatus==2?"已租":"未租"}}</span>
                                </li>
                            </ul>
    
                        </div>
                        <!--  合作信息 -->
                        <div role="tabpanel" class="tab-pane fade" id="companyDetail">
                            <table class="table table-striped table-hover" style="margin-top: 10px;color:#999999;">
                                <thead class="title">
                                    <tr>
                                        <th class="text-center">No.</th>
                                        <th>合同编号</th>
                                        <th style="width: 65px">租户类型</th>
                                        <th>租户名称</th>
                                        <th style="width: 70px;">计租面积<br/>(m
                                    <sup>2</sup>)</th>
                                        <th style="width: 85px;">租金单价<br/> (元/月/m<sup>2</sup>)</th> 
                                        <th style="width: 95px;">计租开始日期</th>
                                        <th style="width: 95px;">计租结束日期</th>
                                        <th>租期（月）</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-if="pageModel.data.length == 0"><td colspan="100" class="text-center">暂无记录</td></tr>
                                    <tr style="color:#333333;cursor: pointer;" ng-repeat="x in pageModel.data" ng-click="lookContract(x.id)">
                                        <td class="text-center">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
                                        <td>{{x.code}}</td>
                                        <td>{{secondPartyType[x.secondPartyType]}}</td>
                                        <td>{{x.secondPartyName}}</td>
                                        <td>{{x.contractArea}}</td>
                                        <td>{{x.initMonthRental|number:2}}</td>
                                        <td>{{x.leaseStart | date : 'yyyy-MM-dd'}}</td>
                                        <td>{{x.leaseEnd | date : 'yyyy-MM-dd'}}</td>
                                        <td>{{x.leaseMonth}}</td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="page-footer clearfix">
                                <ng-include src="'/common/pager.html'" class="ng-scope">
                                <style type="text/css" class="ng-scope">
                                    .pagination>li>span:hover {
                                        background-color: #fff;
                                        cursor: text
                                    }
                                </style>
                        </ng-include>
                    </div>
                        </div>
                    </div>
                </div>
            </div>
</div>
<div class="modal-footer">
    <button class="btn btn-warning" ng-click="cancel()">确认</button>
</div>
