<div ng-controller="faceTrackCtrl">
<face-big-screen map-data="mapData" park-no="parkNo" type="type" show-left="false">
    <big-screen-right>
        <div class="col-lg-12 col-sm-12 b-b b-2x">
            <h4 class="font-bold">陌生人列表</h4>
            <div class="form-group">
                <input type="datetime" style="width:155px;margin-top:5px;display: inline-block" class="form-control Wdate" name="startTime" placeholder="开始时间" ng-model="search.startTime" onchange=""
                       onclick="javascript:WdatePicker({dateFmt:'yyyy-MM-dd HH:mm',maxDate:'%y-%M-%d'})"/>
                <input type="datetime" style="width:155px;margin-top:5px;display: inline-block" class="form-control Wdate" name="endTime" placeholder="结束时间" ng-model="search.endTime" onchange=""
                       onclick="javascript:WdatePicker({dateFmt:'yyyy-MM-dd HH:mm'})"/>
                       <button class="btn btn-default pull-right" ng-click="find(1)"  style="margin-top:5px">查询</button>
            </div>
            <table class="table" style="margin-bottom: 0px">
                <thead class="title">
                <tr>
                    <th>序号</th>
                    <th>轨迹</th>
                    <th>抓拍照片</th>
                    <th>抓拍时间</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-if="pageModel.list.length == 0"><td colspan="100" class="text-center">暂无记录</td></tr>
                <tr ng-repeat="item in pageModel.list" >
                    <td class="text-center">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
                    <td>
                        <a ng-if="!showtk" class="btn-link" href="javascript:;" ng-click="showPosition(item)">定位</a>
                        <a ng-if="showtk" class="btn-link" href="javascript:;" ng-click="showTrack(item)">轨迹</a>
                    </td>
                    <td>
                        <span ng-if="item.photo">
                                <img width="40" height="40" onerror="javascript:this.src='/res/img/detail.png';"
                                     ng-click="showPhoto()"
                                     ng-src="{{processImgUrl(item.photo)}}">
                            </span></td>
                    <td>{{item.time}}</td>
                </tr>
                </tbody>
            </table>
            <div class="page-footer clearfix">
                <ng-include src="'/common/pager.html'"></ng-include>
            </div>
        </div>
    </big-screen-right>

</face-big-screen>
</div>
