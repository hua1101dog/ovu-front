	<!--设置巡查路线的新增修改弹出框  -->
	<div class="modal-header">
		<button type="button" class="close" ng-click="cancel()">×</button>
		<h4 class="modal-title">选择巡查路线</h4>
    </div>
    <div class="modal-body container-fluid">
		<form name="form" novalidate class="form-horizontal">
			<div class="form-group">
				<label class="col-xs-3 control-label">请选择巡查线路：</label>
				<div class="col-xs-8">
					<select ng-hide="item.insOrbit.wayName" class="form-control" ng-model="item.insOrbit.insWayId"
							ng-options="o.insWayId as o.wayName for o in wayList" required>
						<option value="">--请选择--</option>
					</select>
					<input ng-show="item.insOrbit.wayName" type="text" class="form-control"  ng-model="item.insOrbit.wayName" disabled>
				</div>
			</div>
		</form>
	</div>
	<div class="modal-footer">
		<button class="btn btn-primary" ng-click="save(form,n.id)"
			ng-repeat="n in item.status" ng-bind="n.title"></button>
	</div>

