<div class="" ng-controller="alarmMainCtrl as vm">
    <big-screen gaode-data="vm.gaodeData" indoor-data="vm.indoorData" park-no="vm.parkNo" type="vm.type">
        <div class="">
            <!--展示楼栋防盗情况-->
            <div class="col-lg-12 col-sm-12">
                <h4 class="font-bold">防盗报警列表</h4>
                <table class="table">
                    <thead class="title">
                    <tr>
                        <th>设备名称</th>
                        <th>监测参数</th>
                        <th>时间</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-if="!vm.list.length">
                        <td>暂无防盗报警</td>
                    </tr>
                    <!-- <tr ng-repeat="item in vm.list | filter:{is_regular:2}"> -->
                    <tr ng-repeat="item in vm.list" ng-click="vm.checkOne(item.id)"
                        ng-class="{myactive : vm.checked==item.id}">
                        <td>{{item.name}}</td>
                        <td>{{item.val}}</td>
                        <td>{{item.last_time}}</td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </big-screen>
</div>
</div>
<style>
    .myactive{
        color: #18FF00;
    }
</style>