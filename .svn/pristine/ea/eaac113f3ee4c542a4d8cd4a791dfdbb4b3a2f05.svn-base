<div class="modal-header">
    <button type="button" class="close" ng-click="vm.cancel()">×</button>
    <h4 class="modal-title">{{item.id?'编辑':'添加'}}业主</h4>
</div>
<div class="modal-body container-fluid form-horizontal">
    <form name="form" novalidate>
        <div class="form-group">
            <label class="col-xs-2 control-label required">业主地址：</label>
            <div class="col-xs-10 form-inline text-nowrap">
                <select class="form-control" ng-model="item.stageId" required ng-change="geneBuild(item)"  ng-options="o.id as o.text for o in treeData track by o.id">
                    <option value="">--期--</option>
                </select>
                <select class="form-control" ng-model="item.buildId" required ng-change="geneUnit(item)" ng-options="o.id as o.buildName for o in buildList track by o.id">
                    <option value="">--栋--</option>
                </select>
                <select class="form-control" ng-model="item.unitNo" required ng-change="geneGround(item)" ng-options="o as o+'单元' for o in unitList">
                    <option value="">--单元--</option>
                </select>
                <select class="form-control" ng-model="item.groundNo" required ng-change="getHouseList(item)" ng-options="o as o+'楼' for o in groundList">
                    <option value="">--楼--</option>
                </select>
                <select class="form-control" ng-model="item.houseId" required ng-options="o.id as o.houseName for o in houseList">
                    <option value="">--房屋--</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label required">业主电话：</label>
            <div class="col-xs-10">
                <input type="text" class="form-control" ng-model="item.phone" required>
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label required">业主姓名：</label>
            <div class="col-xs-10">
                <input type="text" class="form-control" ng-model="item.name" required>
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label required">业主编码：</label>
            <div class="col-xs-10">
                <input type="text" class="form-control" style="min-width: 150px" required id="{{item.id}}" ng-model="item.ownerNo" name="ownerNo"/>
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label ">业主身份证：</label>
            <div class="col-xs-10">
                <input type="text" class="form-control" ng-model="item.idCard">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label ">业主单位：</label>
            <div class="col-xs-10">
                <input type="text" class="form-control" ng-model="item.ownerUnit">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label ">入住时间：</label>
            <div class="col-xs-10">
                <input type="datetime" class="form-control Wdate" readonly="true" name="REPORT_TIME" ng-model="item.checkinDate" onchange=""
                    onclick="javascript:WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',maxDate:&quot;#F{$('.Wdate').last().val()||'%y-%M-%d'}&quot;})">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">备注：</label>
            <div class="col-xs-10">
                <textarea type="text" style="height:150px;resize:vertical" class="form-control" ng-model="item.comment" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">提示：</label>
            <div class="col-xs-10 mt7">
                备注最多输入200字。
            </div>
        </div>

    </form>
</div>
<div class="modal-footer">
    <button class="btn btn-primary" ng-click="vm.save(form,item)">确定</button>
    <button class="btn btn-warning" ng-click="vm.cancel()">取消</button>
</div>