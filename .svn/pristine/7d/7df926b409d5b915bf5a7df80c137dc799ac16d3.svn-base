<div class="modal-header">
    <button type="button" class="close" ng-click="cancel()">×</button>
    <h4 class="modal-title">{{ruleTitle}}</h4>
</div>
<div class="modal-body container-fluid form-horizontal">
    <table class="table table-striped table-hover" style="margin-top: 10px;">
        <thead class="title">
            <tr>
                <th class="text-center">NO.</th>
                <th>姓名</th>
                <th>识别照片</th>
                <th>抓拍位置</th>
                <th>职务</th>
                <th>联系电话</th>
                <th>进入时间</th>
            </tr>
        </thead>
        <tbody class="table_tbody_box">
            <tr ng-if="staffChildList.length == 0">
                <td colspan="100" class="text-center">暂无数据</td>
            </tr>
            <tr ng-repeat="item in staffChildList" class="ng-scope">
                <td class="text-center">{{$index + 1}}</td>
                <td class="">{{item.subject.name}}</td>
                <td class=""> <img class="tableImg" ng-src="{{item.avatarPhoto}}" ng-click="showBigImg(item.avatarPhoto)" alt=""> </td>
                <td class="">{{item.screen.camera_position}}</td>
                <td class="">{{item.subject.title}}</td>
                <td class="">{{item.subject.phone}}</td>
                <td class="">{{item.entryDate}}</td>
            </tr>
        </tbody>
    </table>
    <ng-include src="'/common/pager3.html'" class="ng-scope">
        <style type="text/css" class="ng-scope">
            .pagination>li>span:hover {
                background-color: #fff;
                cursor: text
            }
        </style>
    </ng-include>
</div>
