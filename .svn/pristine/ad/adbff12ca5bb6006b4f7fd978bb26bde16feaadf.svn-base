            <div class="panel panel-default" ng-show="pageStatus == 1">
                <div class="panel-heading" style="color:#999999;">复核信息</div>
                <div class="panel-body">
                    <table style="color:#999999;border: 1px solid #ddd;width: 100%;">
                        <tbody>
                            <tr style="color:#333333; height: 30px;">
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">复核人：</td>
                                <td style="border: 1px solid #ddd;">{{return.checkUserName}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">复核日期：</td>
                                <td style="border: 1px solid #ddd;">{{return.checkTime}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">付款账户：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.payAccount}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">付款人：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.payUser}}</td>
                            </tr>
                            <tr style="color:#333333; height: 30px;">
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">付款备注：</td>
                                <td style="border: 1px solid #ddd;" colspan="7">{{receivable.receiveRemark}}</td>
                            </tr>
                        </tbody>
                    </table>
                    <!--<ul class="basaMsg_ul">
