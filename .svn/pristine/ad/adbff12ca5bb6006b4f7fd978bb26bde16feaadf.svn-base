<body>
    <div id="angularId" ng-controller="refundExamineCtrl">
        <div class="page-title">
            <div class="title_left">
                <h3 ng-show="pageStatus == 1">复核查看</h3>
                <h3 ng-show="pageStatus == 2">退款审批</h3>
                <h3 ng-show="pageStatus == 3">审批查看</h3>
            </div>
        </div>
        <div class="row">
            <div class="panel panel-default" ng-show="(pageStatus == 2)||(pageStatus == 3)">
                <div class="panel-heading" style="color:#999999;">收款信息</div>
                <div class="panel-body">
                    <table style="color:#999999;border: 1px solid #ddd;width: 100%;">
                        <tbody>
                            <tr style="color:#333333; height: 30px;">
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">租户名称：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.secondPartyName}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">收款日期：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.receiveDate}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">收款方式：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.paymentMethod| payment}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">应缴总额：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.payableAmount| number:2}}</td>
                            </tr>
                            <tr style="color:#333333; height: 30px;">
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">收款金额：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.receiveAmount| number:2}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">收款账户：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.receiveAccount}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">收款银行：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.receiveBank}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">收款人：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.receiveUserName}}</td>
                            </tr>
                            <tr style="color:#333333; height: 30px;">
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">是否开发票：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.isInvoice|invoice1}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">发票号：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.invoiceNo}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">付款账号：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.payAccount}}</td>
                                <td style="border: 1px solid #ddd;color: #999999;font-weight: bold;">付款人：</td>
                                <td style="border: 1px solid #ddd;">{{receivable.payUser}}</td>
                            </tr>
                        </tbody>
                    </table>
