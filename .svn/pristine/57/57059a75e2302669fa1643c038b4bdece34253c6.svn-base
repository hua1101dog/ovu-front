	<!--供应商管理的新增修改弹出框  -->
	<div class="modal-header">
		<button type="button" class="close" ng-click="cancel()">×</button>
		<h4 class="modal-title">差评工单列表</h4>
    </div>
    <div class="modal-body container-fluid">
    	   <!-- <div class="x_panel" >
				 <div class="form-inline">
					<div class="form-group">
						<label>回访人</label>
						<input type="text" ng-model="search.emtId"   class="form-control" />
					</div>
					<button class="btn btn-default" ng-click="find(1)">查询</button>
				</div> 
			</div> -->
			<div class="panel panel-default">
				<div class="panel-body">
					<table class="table table-striped table-hover table-condensed" style="margin-top: 10px;">
						<thead class="title">
							<tr>
								<th class="text-center">NO</th>
								<th>工单编号</th>
								<th>工单名称</th>
								<th>工单类型</th>
								<th>工作分类</th>
								<th >位置</th>
								<th>发起人</th>
								<th>执行人</th>
								<th>管理人</th>
								<th>创建时间</th>
								<th>执行时间</th>
								<th>评分</th>
								<th>督办人</th>
							</tr>
						</thead>
						<tbody>
							<tr ng-if="pageModel.data.length == 0"><td colspan="100" class="text-center">暂无记录</td></tr>
							<tr ng-repeat="item in pageModel.data">
								<td class="text-center">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
								<td>{{item.ID}}</td>
								<td><a  href='javascript:void(0)' class="btn-link" ng-style="{'text-decoration':item.DATA_STATUS=='0'?'line-through':''}" ng-click='showWorkUnitDetail(item.ID)'>{{item.WORKUNIT_NAME}}</a></td>
								<td>{{item.WORKUNIT_TYPE|keyToValue:workunitTypeDict}}</td>
								<td>{{item.WORKTYPE_NAME}}</td>
								<td>{{item.reportLoc}}</td>
								<td>{{item.SOURCE_PERSON_NAME}}</td>
								<td>{{item.EXEC_PERSON_NAME}}</td>
								<td>{{item.MANAGE_PERSON_NAME}}</td>
								<td>{{item.CREATE_DATE}}</td>
								<td>{{item.EXEC_DATE}}</td>
								<td>{{item.EVALUATE_SCORE}}</td>
								<!-- <td>{{item.UNIT_STATUS|keyToValue:unitStatusDict}}</td> -->
								<td>{{item.SUPERVISE_PERSON_NAMES}}</td>
							</tr>
						</tbody>
			 		</table>
				 	 <div class="page-footer clearfix">
						  <ng-include src="'/common/pager.html'"></ng-include>
					 </div>
				</div>
			</div>
	</div>
	<div class="modal-footer">
		<button class="btn btn-warning" ng-click="cancel()">取消</button>
	</div>

