<link rel="stylesheet" href="dataCollection/companyDatabase/companyIndex.css">
<div class="modal-header">
    <button type="button" class="close" ng-click="cancel()">×</button>
    <h4 class="modal-title">查看详情</h4>
</div>
<div class="modal-body container-fluid form-horizontal ng-scope">
    <ul class="companyTab" style="float: left;" id="companyTab" class="nav nav-tabs">
        <li class="active">
            <a href="#background" data-toggle="tab">企业背景</a>
        </li><br>
        <li><a href="#development" data-toggle="tab">企业发展</a></li><br>
        <li><a href="#information" data-toggle="tab">经营信息</a></li>
    </ul>
    <div style="float: left;width: 90%;margin-left: 20px" class="tab-content">
        <div class="tab-pane fade in active" id="background">
            <div  class="backgroundTab">
                <ul id="backgroundTab" class="nav nav-tabs">
                    <li class="active">
                        <a href="#background1" data-toggle="tab">基本信息</a>
                    </li>
                    <li><a href="#background2" data-toggle="tab">主要人员</a></li>
                    <li><a href="#background3" data-toggle="tab">股东信息</a></li>
                    <li><a href="#background4" data-toggle="tab">关联关系</a></li>
                    <li><a href="#background5" data-toggle="tab">对外投资</a></li>
                    <li><a href="#background6" data-toggle="tab">变更记录</a></li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane fade in active" id="background1"  ng-controller="background1">
                        <form name="form" novalidate>
					        <div class="form-group">
					            <label class="col-md-2 control-label">公司名称:</label>
					            <span class="col-md-7" style="padding-top:8px;">{{item.name}}</span>
					        </div>
					        <div class="form-group">
					            <label class="col-md-2 control-label">英文名称:</label>
					            <span class="col-md-7" style="padding-top:8px;">{{item.englishName}}</span>
					        </div>
					        <div class="form-group">
					            <label class="col-md-2 control-label">公司行业:</label>
					            <span class="col-md-7" style="padding-top:8px;">{{item.industryName}}</span>
					        </div>
					        <div class="form-group">
					            <label class="col-md-2 control-label">公司地址:</label>
					            <span class="col-md-7" style="padding-top:8px;">{{item.registrationAddress}}</span>
					        </div>
					        <div class="form-group">
					           <label class="col-md-2 control-label">企业组织架构:</label>
					           <button class="btn btn-info" ng-click="showCorStr()">查看详情</button>
					        </div>
					        <div class="panel panel-default" style="margin-top: 10px;">
					            <div class="panel-heading" style="color:#999999;">
					                公司工商信息
					            </div>
					            <div class="panel-body">
					                <div class="form-group">
					                    <label class="col-md-2 control-label">工商注册号:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.registrationNumber}}</span>
					                    <label class="col-md-2 control-label">纳税人识别号:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.taxpayerNumber}}</span>
					                </div>
					                <div class="form-group">
					                    <label class="col-md-2 control-label" style="padding-left:0px;width: 131px;">统一社会信用代码:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.creditCode}}</span>
					                    <label class="col-md-2 control-label">成立日期:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.registeredDate | date:"yyyy-MM-dd"}}</span>
					                </div>
					                <div class="form-group">
					                    <label class="col-md-2 control-label">组织机构代码:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.organizationCode}}</span>
					                    <label class="col-md-2 control-label">经营期限:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.businessTerm}}</span>
					                </div>
					                <div class="form-group">
					                    <label class="col-md-2 control-label">企业类型:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.companyType | convertCompanyType}}</span>
					                    <label class="col-md-2 control-label">登记机关: </label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.registerDepartment}}</span>
					                </div>
					                <div class="form-group">
					                    <label class="col-md-2 control-label">经营状态:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.companyStatus | convertCompanyStatus}}</span>
					                    <label class="col-md-2 control-label">注册地址:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.registrationAddress}}</span>
					                </div>
					                <div class="form-group">
					                    <label class="col-md-2 control-label">注册资本:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.registeredMoney}}(万元)</span>
					                    <label class="col-md-2 control-label">法人代表: </label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.corporate}}</span>
					                </div>
					                <div class="form-group">
					                    <label class="col-md-2 control-label">核准日期:</label>
					                    <span class="col-md-3" style="padding-top:8px;">{{item.approvalDate | date:"yyyy-MM-dd"}}</span>
					                </div>
					                <div class="form-group">
					                    <label class="col-md-2 control-label">经营范围:</label>
					                    <span class="col-md-10" style="padding-top:8px;">{{item.businessScope}}</span>
					                </div>
					            </div>
					        </div>
				        </form>
                    </div>
                    <div class="tab-pane fade" id="background2" ng-controller="background2">
                        <table class="table table-striped table-hover">
			                <thead class="title">
			                    <tr class="text-center">
			                        <th class="text-center">序号</th>
			                        <th class="text-center">姓名</th>
			                        <th class="text-center">联系方式</th>
			                        <th class="text-center">职位</th>
			                        <th class="text-center">部门</th>
			                    </tr>
			                </thead>
			                <tbody>
			                <tr ng-if="pageModel.data.length == 0 || !pageModel.data"><td colspan="100" class="text-center">暂无记录</td></tr>
			                <tr ng-repeat="stuff in pageModel.data" class="ng-scope">
			                    <td class="text-center ng-binding">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
			                    <td class="text-center">{{stuff.name}}</td>
			                    <td class="text-center">{{stuff.mobile}}</td>
			                    <td class="text-center">{{stuff.positionName}}</td>
			                    <td class="text-center">{{stuff.departmentName}}</td>
			                </tr>
			                </tbody>
			            </table>
			            <!--分页-->
			            <div class="page-footer clearfix">
			                <ng-include src="'/common/pager.html'" class="ng-scope">
			                    <style type="text/css" class="ng-scope">
			                        .pagination>li>span:hover{background-color: #fff;cursor:text}
			                    </style>
			                </ng-include>
			            </div>
                    </div>
                    <div class="tab-pane fade" id="background3" ng-controller="background3">
