                                <tr class="text-center">
                                    <th class="col-md-1 text-center">序号</th>
                                    <th class="col-md-2 text-center">发布时间</th>
                                    <th class="col-md-2 text-center">招聘职位</th>
                                    <th class="col-md-2 text-center">薪资</th>
                                    <th class="col-md-1 text-center">工作经验</th>
                                    <th class="col-md-2 text-center">招聘人数</th>
                                    <th class="col-md-2 text-center">所在城市</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-if="pageModel.data.length == 0 || !pageModel.data"><td colspan="100" class="text-center">暂无记录</td></tr>
                                <tr ng-repeat="recruit in pageModel.data" class="ng-scope">
                                    <td class="text-center ng-binding">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
                                    <td class="text-center">{{recruit.beginTime | date:"yyyy-MM-dd"}}</td>
                                    <td class="text-center">{{recruit.position}}</td>
                                    <td class="text-center">{{recruit.salary}}</td>
                                    <td class="text-center">{{recruit.workExperience}}</td>
                                    <td class="text-center">{{recruit.number}}</td>
                                    <td class="text-center">{{recruit.city}}</td>
                                </tr>
                            </tbody>
                        </table>
                        </div>
                        <!--分页-->
                        <div class="page-footer clearfix">
                            <ng-include src="'/common/pager.html'" class="ng-scope">
                                <style type="text/css" class="ng-scope">
                                    .pagination>li>span:hover{background-color: #fff;cursor:text}
                                </style>
                            </ng-include>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="information3" ng-controller="information3">
                        <table class="table table-striped table-hover" >
                            <thead class="title">
                                <tr class="text-center">
                                    <th class="col-md-1 text-center">序号</th>
                                    <!-- <th class="text-center">图标</th> -->
                                    <th class="col-md-5 text-center">产品名称</th>
                                    <th class="col-md-2 text-center">产品简称</th>
                                    <th class="col-md-2 text-center">产品分类</th>
                                    <th class="col-md-2 text-center">领域</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-if="pageModel.data.length == 0 || !pageModel.data"><td colspan="100" class="text-center">暂无记录</td></tr>
                                <tr ng-repeat="product in pageModel.data" class="ng-scope">
                                    <td class="text-center ng-binding">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
                                    <!-- <td class="text-center">{{product.productIcon}}</td> -->
                                    <td class="text-center">{{product.productName}}</td>
                                    <td class="text-center">{{product.abbreviation}}</td>
                                    <td class="text-center">{{product.productType}}</td>
                                    <td class="text-center">{{product.domain}}</td>
                                </tr>
                            </tbody>
                        </table>
                        <!--分页-->
                        <div class="page-footer clearfix">
                            <ng-include src="'/common/pager.html'" class="ng-scope">
                                <style type="text/css" class="ng-scope">
                                    .pagination>li>span:hover{background-color: #fff;cursor:text}
                                </style>
                            </ng-include>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button class="btn btn-warning" ng-click="cancel()">关 闭</button>
</div>
<script>
    $(function () {
         // 初始化显示的选项卡及样式
        $('#companyTab li:eq(0) a').tab('show');
        $('#backgroundTab li:eq(0) a').tab('show');
        $('#informationTab li:eq(0) a').tab('show');
        $('#developmentTab li:eq(0) a').tab('show');
        $('.companyTab li:eq(0)').addClass('addUnderline');

         // 点击显示的选项卡样式
        $('.companyTab li').click(function () {
            $('.companyTab li').removeClass('addUnderline');
            $(this).addClass('addUnderline');
        })
    });
</script>