	                            <tr ng-if="pageModel.data.length == 0 || !pageModel.data"><td colspan="100" class="text-center">暂无记录</td></tr>
	                            <tr ng-repeat="shareholder in pageModel.data" class="ng-scope">
	                                <td class="text-center ng-binding">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
	                                <td class="text-center">{{shareholder.name}}</td>
	                                <td class="text-center">{{shareholder.rate}}%</td>
	                                <td class="text-center">{{shareholder.contribution}}万元</td>
	                            </tr>
                            </tbody>
