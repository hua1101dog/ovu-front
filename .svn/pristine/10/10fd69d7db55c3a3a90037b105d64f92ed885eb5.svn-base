			                    </tr>
			                </thead>
			                <tbody>
				                <tr ng-if="pageModel.data.length == 0 || !pageModel.data"><td colspan="100" class="text-center">暂无记录</td></tr>
				                <tr ng-repeat="company in pageModel.data" class="ng-scope">
				                    <td class="text-center ng-binding">{{$index + 1+pageModel.pageSize*pageModel.pageIndex}}</td>
				                    <td class="text-center">{{company.name}}</td>
				                    <td class="text-center">{{company.companyType | convertCompanyType}}</td>
				                    <td class="text-center">{{company.corporate}}</td>
				                    <td class="text-center">{{company.registeredMoney}}(万元)</td>
				                    <td class="text-center">{{company.registeredDate| date:"yyyy-MM-dd"}}</td>
				                    <td class="text-center">{{company.companyStatus | convertCompanyStatus}}</td>
				                    <td class="text-center">{{company.rate}}%</td>
				                    <td class="text-center">{{company.ownership | convertOwnership}}</td>
				                </tr>
			                </tbody>
			            </table>
			            <!--分页-->
