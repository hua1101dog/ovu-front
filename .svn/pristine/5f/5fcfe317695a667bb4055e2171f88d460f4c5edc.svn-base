<div ng-controller="partnershipagreementCtrl">
    <div class="page-title">
        <div class="title_left">
            <h3>入伙协议管理</h3>
            <div ng-if="search.isGroup">
                <label class="control-label"><i class="fa fa-map-marker" style="font-size: 15px;margin-right:5px"></i><a role="button" ng-click="findPark(search,callback)">{{search.PARK_NAME?search.PARK_NAME:'请选择项目'}} </a></label>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="form-inline">
                    <div class="form-group">
                        <input type="text" class="form-control" style="width:200px" ng-model="search.agreementTitle" placeholder="请输入标题" />
                        <button class="btn btn-default" ng-click="find()">查询</button>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <span style="display:inline-block;">入伙协议列表</span>
                    <button class="btn btn-default pull-right" ng-disabled="!hasChecked(pageModel)" style="" ng-click="delAll()">批量删除</button>
                    <button class="btn btn-primary pull-right" ng-disabled="!search.deptId" ng-click="showModal(item)" style="">+新增协议</button>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-hover" style="margin-top:10px">
                        <thead>
                            <tr>
                                <!-- <th><input type="checkbox" class="checkAll"/></th>  -->
                                <th class="text-center" style="width:20px"><span role="button" ng-click="checkAll(pageModel)">
								<i class="glyphicon" ng-class="{'glyphicon-check':pageModel.checked,'glyphicon-unchecked':!pageModel.checked}"></i></span></th>
                                <th>标题</th>
                                <th>创建时间</th>
                                <th>操作</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-if="!pageModel.data || pageModel.data.length == 0">
                                <td colspan="100" class="text-center">暂无记录</td>
                            </tr>
                            <tr ng-repeat="item in pageModel.data">
                                <td><i role="button" ng-click="checkOne(item,pageModel)" ng-class="{'glyphicon-check':item.checked,'glyphicon-unchecked':!item.checked}"
                                        class="glyphicon"></i></td>
                                <td>{{item.agreementTitle}}</td>
                                <td>{{item.createTime}}</td>
                                <td>
                                    <a href="javascript:void(0)" ng-click="showModal(item)" class="btn btn-xs"><span class="fa fa-edit"></span>编辑</a>
                                    <a href="javascript:void(0)" ng-click="del(item)" class="btn btn-xs"><span class="fa fa-trash"></span>删除</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="page-footer clearfix">
                        <ng-include src="'/common/pager.html'"></ng-include>
                    </div>
                </div>
            </div>
           
        </div>
    </div>
</div>
<style>
    table tr th,
    table tr td {
        text-align: left;
    }
</style>
<script src="/view/join/partnershipagreementService.js"></script>