<!-- //摄像机列表弹出框 -->
<div class="modal-header">
    <button type="button" class="close" ng-click="cancel()">×</button>
    <h4 class="modal-title">摄像机列表</h4>
</div>
<div class="modal-body container-fluid form-horizontal">
   
    <div class="panel panel-default">
        <div class="panel-heading">摄像机列表
        </div>
        <div class="panel-body">
            <table class="table table-hover" style="margin-top: 10px;">
                <thead class="title">
                    <tr>
                        <th class="text-center">NO</th>
                        <th>摄像机编码</th>
                        <th>摄像机名称</th>
                       <th>操作</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-if="pageModel.data.length == 0">
                        <td colspan="100" class="text-center">暂无记录</td>
                    </tr>
                    <tr ng-repeat="item in pageModel.data" ng-class="{'bg-success':item.checked}">
                        <td class="text-center">{{$index + 1*1}}</td>
                        <td>{{item.regi_code}}</td>
                        <td>{{item.equip_simple_name}}</td>
                    
                        <td>
                            <a href="javascript:void(0)" ng-click="playVideo('',item.regi_code,'playback',startTime,endTime)" class="btn btn-xs"><span class="fa fa-camera"></span>播放</a>
                        </td>
  
                    </tr>
                </tbody>
            </table>
            <div class="page-footer clearfix">
                <ng-include src="'/common/pager.html'"></ng-include>
            </div>
        </div>
    </div>
</div>
