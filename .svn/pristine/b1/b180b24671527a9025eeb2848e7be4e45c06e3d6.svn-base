.view_body {
    padding: 0px 15px;
    /* overflow-y: auto; */
    /* height: 100%; */
    background: #F7F7F7;
}

.footer {
    margin-bottom: 0px;
    padding: 15px 20px;
    position: absolute;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 0;
}
.menuSearch{
    width:100%;
    height:100%;
    position: absolute;
    top:0px;
    left:0px;
    z-index:1000;
}

table,
tr,
td,
ul,
li,
div,
dl,
dt,
dd,
p,
h1,
h2,
h3,
h4,
h5,
b,
input {
    padding: 0px;
    list-style: none;
    font-family: 'Microsoft YaHei', Arial, SimSun;
    
}
table,
tr,
td,
ul,
li,
div,
dl,
dt,
dd,
p,
h1,
h2,
