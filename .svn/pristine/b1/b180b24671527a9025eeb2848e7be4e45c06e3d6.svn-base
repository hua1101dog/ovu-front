@charset "utf-8";

/* CSS Document */

html {
    font-family: sans-serif;
    height: 100%;
    min-height: 100%;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%
}

body {
    font: 12px/1.5 'Microsoft YaHei', Arial, SimSun;
    font-family: 'Microsoft YaHei', Arial, SimSun;
    font-size: 12px;
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    font-stretch: normal;
    line-height: 1.5;
    min-height: 100%;
    height: 100%;
    min-width: 1024px;
   /* height: auto!important;*/
    overflow-x: hidden;
    position: relative;
}
textarea{
    resize: none
}
#mainFrame {}

.layui-layer-tips {
    margin-left: -180px!important;
}

*:focus { outline: none !important } 

