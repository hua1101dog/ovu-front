<!-- 设备房 -->
<div ng-controller="firectrl">
    <div class="page-title">
        <div class="title_left">
            <h4>消防事件  >></h4>
        </div>
    </div>
    <div class="x_panel" style="padding:0;">
        <div class="p-header">
            <div style="font-size: 0;display: flex;flex-direction: row;align-items: center" >
                <input type="text" class="p-ipt" placeholder="设备名称" ng-model="search.equipName">
                <button class="search-00" ng-click="search00()"><img style="width: 24px;height: 24px;" src="../../image/search.png" alt=""></button>
            </div>
            <div style="display: flex;flex-direction: row;align-items: center;margin-left: 35px">
                <input type="checkbox" class="checkbox-00" ng-model="eqtp1">
                <span class="checkbox-00-lab">烟雾传感器</span>
            </div>
            <div style="display: flex;flex-direction: row;align-items: center;margin-left: 35px">
                <input type="checkbox" class="checkbox-00" ng-model="eqtp2">
                <span class="checkbox-00-lab">温湿度传感器</span>
            </div>
            <div style="display: flex;flex-direction: row;align-items: center;margin-left: 35px">
                <input type="checkbox" class="checkbox-00" ng-model="eqtp3">
                <span class="checkbox-00-lab">液位传感器</span>
            </div>
             <div style="display: flex;flex-direction: row;align-items: center;margin-left: 35px">
                <input type="checkbox" class="checkbox-00" ng-model="eqtp4">
                <span class="checkbox-00-lab">消防主机</span>
            </div>
           

        </div>
        <div style="padding: 20px;max-height: 600px;overflow-y: scroll;overflow-x: hidden">
            <table class="table table-striped table-hover" style="margin-top: 10px;">
                <thead>
                <tr>
                    <th>工单名称</th>
                    <th>关联设备</th>
                    <th>发生时间</th>
                 <!--   <th>摄像头</th>  -->
                    <th>操作</th>

                </tr>
                </thead>
                <tbody>

                <tr ng-if="!pageModel.totalCount">
                    <td colspan="100" class="text-center">暂无记录</td>
                </tr>
                <tr ng-repeat="item in pageModel.data ">
                    <td><a  href='javascript:void(0)' class="btn-link" ng-click='showWorkUnitDetail(item.id)'>{{item.workunit_name}}</a></td>
                    <td>{{item.equipment_name}}</td>
                    <td>{{item.create_date}}</td>
                   <!--  <td>{{item.regi_code=='否'?item.regi_code:'是'}}</td> -->
                    <td>
                        <a href='javascript:void(0)' class='btn btn-xs btn-link' ng-click="showDetail(item)">查看详情</a>
                    </td>
                </tr>
                </tbody>
            </table>

            <div class="page-footer clearfix">
                <ng-include src="'/common/pager.html'"></ng-include>
            </div>
        </div>
    </div>
</div>

<style>
    .page-title{
        width: 100%;
        height: 60px;
        padding: 10px 30px;
        background-color: #E2F0FF;
    }
    .page-title h4{
        font-weight: bold;
        color: rgba(0,120,255,1);
    }

    .title{
        background:rgba(222,232,248,1);
        color: #0078FF
    }
    .p-header{
        background: #fff;
        display: flex;
        flex-direction: row;
        margin-top: 15px;
        margin-left: 20px;
    }
    .p-ipt{
        width:200px;
        height:30px;
        background:rgba(246,246,246,1);
        border:1px solid rgba(231,231,231,1);
        box-shadow:0px 1px 1px 0px rgba(0, 0, 0, 0.08);
        font-size: 15px;
    }
    .search-00{
        display: inline-block;
        width:40px;
        height:30px;
        background:rgba(0,120,255,1);
    }
    .checkbox-00{
        width: 20px;
        height: 20px;
    }
    .checkbox-00-lab{
        font-size:14px;
        font-family:MicrosoftYaHei;
        font-weight:400;
        color:rgba(102,102,102,1);
    }
    table tr th,
        table tr td {
            text-align: left;
        }
</style>



