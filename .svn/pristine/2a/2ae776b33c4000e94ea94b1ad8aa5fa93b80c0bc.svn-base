		<div class="modal-header">
			<button type="button" class="close" ng-click="cancel()">×</button>
			<h4 class="modal-title">{{item.ID?'编辑':'添加'}}业主</h4>
	    </div>
	    <div class="modal-body container-fluid form-horizontal">
			<form name="form" novalidate>
					<div class="form-group">
						<label class="col-xs-2 control-label required">业主姓名：</label>
						<div class="col-xs-4">
							<input type="text" class="form-control" required ng-model="item.NAME" name="NAME" />
						</div>
						<label class="col-xs-2 control-label required">业主电话：</label>
						<div class="col-xs-4">
							<input type="text" class="form-control" required name="PHONE" ng-model="item.PHONE" />
						</div>
					</div>
					<div class="form-group">
						<label class="col-xs-2 control-label ">身份证号：</label>
						<div class="col-xs-4">
							<input type="text" class="form-control" ng-model="item.ID_NUMBER" name="ID_NUMBER" />
						</div>
						<label class="col-xs-2 control-label">入住日期：</label>
						<div class="col-xs-4">
							<input type="text" class="form-control Wdate" name="MOVEIN_DATE" ng-model="item.MOVEIN_DATE"
								   onchange="" onclick="javascript:WdatePicker({dateFmt:'yyyy-MM-dd',maxDate:'%y-%M-%d'})" />
						</div>
					</div>
				<div class="form-group">
					<label class="col-xs-2 control-label required">房屋：</label>
					<div class="col-xs-10 form-inline text-nowrap">
						<select class="form-control" ng-model="item.STAGE" required ng-options="o as o.stageName for o in treeData track by o.stageId" >
							<option value="">--期--</option>
						</select>
						<select class="form-control" ng-model="item.FLOOR" required ng-change="geneUnit(item)" ng-options="o as o.floorName for o in item.STAGE.nodes track by o.floorId" >
							<option value="">--栋--</option>
						</select>
						<select class="form-control" ng-model="item.unit_no" required ng-change="geneGround(item)" ng-options="o.unit_no as o.unit_no+'单元' for o in unitList" >
							<option value="">--单元--</option>
						</select>
						<select class="form-control" ng-model="item.ground_no" required ng-change="getHouseList(item)" ng-options="o.ground_no as o.ground_no+'楼' for o in groundList" >
							<option value="">--楼--</option>
						</select>
						<select class="form-control" ng-model="item.HOUSE_ID" required ng-options="o.ID as o.HOUSE_NAME disable when o.ID=='0' for o in houseList"  >
							<option value="">--房号--</option>
						</select>
					</div>
				</div>
				<div class="form-group" >
					<label class="col-xs-2 control-label">备注：</label>
					<div class="col-xs-10">
						<textarea type="text" style="height:150px;resize:vertical" class="form-control" ng-model="item.COMMENT"   />
					</div>
				</div>
			</form>
		</div>
		<div class="modal-footer">
			<button class="btn btn-primary" ng-click="save(form,item)">确定</button>
			<button class="btn btn-warning" ng-click="cancel()">取消</button>
		</div>

