<div controller="enterpriseInfoCtrl" ng-init="find(1)">
	<div class="info-title">
		<button type="button" class="close" ng-click="vm.cancel()">×</button>
		<span>企业信息</span>
	</div>
	<div class="container-fluid">
		<div class="x_content" style="max-height: 500px; overflow: auto;">
			<div class="panel-body popup-table">
				<table class="table table-striped table-hover"
					style="margin-top: 10px; color: #999999;">
					<thead class="title">
						<tr>
							<th class="text-center no-common">序号</th>
							<th>企业名称</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-if="!pageModel.data || pageModel.data.length == 0">
							<td colspan="100" class="text-center">暂无记录</td>
						</tr>
						<tr ng-repeat="item in pageModel.data track by $index"
							style="color: #333333;">
							<td class="text-center no-common">{{$index +
									1+pageModel.pageSize*pageModel.pageIndex}}</td>
							<td><a href='javascript:void(0)'
								ng-click='vm.gotoEnterpriseTab(item)' class='btn btn-xs'
								style="color: #5692ea;">{{item.companyName}}</a></td>
						</tr>
					</tbody>
				</table>
				<div class="page-footer clearfix">
						<ng-include src="'/common/pager.html'" class="ng-scope">
						<style type="text/css" class="ng-scope">
							.pagination>li>span:hover {
								background-color: #fff;
								cursor: text
							}
							</style>
						</ng-include>
					</div>
			</div>
		</div>
	</div>
</div>
