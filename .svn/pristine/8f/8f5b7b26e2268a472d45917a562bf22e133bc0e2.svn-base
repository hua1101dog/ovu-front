<div class="modal-header">
    <button type="button" class="close" ng-click="cancel()">×</button>
    <h4 class="modal-title">合同查阅</h4>
</div>
<div class="modal-body container-fluid form-horizontal col-md-6 col-lg-6"></div>
<div class="modal-body container-fluid form-horizontal col-md-6 col-lg-6">
    <form name="form" novalidate>
        <div class="form-group">
            <label class="control-label required col-md-3 col-xs-12">合同分类:</label>
            <select class="form-control col-md-4 col-xs-12" ng-model="classifyName" required ng-options="o[0] as o[1] for o in msg">
                <option value="">--请选择--</option> 
            </select>
        </div>
        <div class="form-group">
            <label class="control-label required col-md-3 col-sm-4 col-xs-12">合同模版:</label>
            <select class="form-control col-md-4 col-xs-12" ng-model="classifyName" required ng-options="o[0] as o[1] for o in msg">
                <option value="">--请选择--</option> 
            </select>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label required">合同名称:</label>
            <div class="col-xs-8">
                <input type="text" class="form-control" required ng-model="item.classifyName" name="classifyName" placeholder="请输入分类名称">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label required">到期时间:</label>
            <div class="col-xs-8">
                <input type="text" class="form-control" required ng-model="item.classifyCode" name="classifyCode" placeholder="请输入分类编码">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label required">合同金额:</label>
            <div class="col-xs-8">
                <input type="text" class="form-control" required ng-model="item.classifyCode" name="classifyCode" placeholder="请输入分类编码">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label required">是否独立合同:</label>
            <div class="col-xs-8">
                <label class="radio-inline">
                    <input type="radio" value="1" ng-model="item.classifyType"/>是
                </label>
                <label class="radio-inline">
                    <input type="radio" value="2" ng-model="item.classifyType"/>否
                </label>
            </div>
        </div>
        <div class="form-group col-md-3 col-xs-8">
            <label class="control-label required">父合同名称:</label>
            <select class="form-control" ng-model="classifyName" required ng-options="o[0] as o[1] for o in msg">
                <option value="">--请选择--</option> 
            </select>
        </div>
       
    </form>
</div>
<div class="modal-footer">
    <button class="btn btn-primary" ng-click="save(form,item)">打印</button>
    <button class="btn btn-warning" ng-click="cancel()">返回</button>
</div>