	<!--处理详情弹出框  -->
	<div class="modal-header">
		<button type="button" class="close" ng-click="vm.cancel()">×</button>
		<h4 class="modal-title">处理详情</h4>
    </div>
    <div class="modal-body container-fluid">
		<form name="form" novalidate class="form-horizontal" >
			<div class="form-group">
				<label class="col-sm-3 col-xs-3 control-label ">处理人：</label>
				<label class="col-sm-7 col-xs-7 control-label ">处理人：</label>
			</div>
			<div class="form-group">
				<label class="col-sm-3 col-xs-3 control-label ">处理时间：</label>
				<label class="col-sm-7 col-xs-7 control-label ">处理人：</label>
			</div>
			<div class="form-group">
				<label class="col-sm-3 col-xs-3 control-label ">处理类型：</label>
				<label class="col-sm-7 col-xs-7 control-label ">处理人：</label>
			</div>
			<div class="form-group">
				<label class="col-xs-3 control-label required">证明图片：</label>
				<div class="col-xs-8">
					<div class="thumbnail" style="height: auto;">
						<img onerror="javascript:this.src='/res/img/upload.jpg';"
							 ng-click="showPhoto(vm.item)"
							 ng-src="{{'/ovu-pcos/'+vm.item.imgPath.substr(vm.item.imgPath.indexOf('document'))}}">
					</div>
				</div>
			</div>
		</form>
	</div>
	<div class="modal-footer">
		<button class="btn btn-warning" ng-click="vm.cancel()">取消</button>
	</div>

