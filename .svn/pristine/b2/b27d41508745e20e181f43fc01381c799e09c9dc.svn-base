<div class="" ng-controller="liftMainCtl as vm" style="height:100%;">
    <div ng-if="vm.fmapId == 'csrjy'" style="height: 100%;position:relative;">
        <h1 class="text-center" style="position:absolute;top:50%;left:50%;transform: translate(-50%, -50%)">开发中...</h1>
    </div>
    <big-screen ng-if="vm.fmapId != 'csrjy'" gaode-data="vm.gaodeData" indoor-data="vm.indoorData" park-no="vm.parkNo" type="vm.type">
        <div class="">
            <!--园区电梯情况-->
            <div ng-show="vm.showPark">
                <div class="col-lg-12 col-sm-12 b-b b-2x">
                    <h4 class="font-bold">园区电梯概况</h4>
                    <div class="col-lg-3 col-sm-3 text-center">
                        <h3>{{vm.rightData.total}}</h3>
                        <div>电梯总数</div>
                    </div>
                    <div class="col-lg-3 col-sm-3 text-center">
                        <h3>{{vm.rightData.total-vm.rightData.broken_num}}</h3>
                        <div>正常</div>
                    </div>
                    <div class="col-lg-3 col-sm-3 text-center">
                        <h3>{{-(-vm.rightData.broken_num - vm.rightData.fail_num)}}</h3>
                        <div class="red">异常</div>
                    </div>
                    <div class="col-lg-3 col-sm-3 text-center m-b-md">
                        <h3>{{vm.rightData.max_regular_days}}</h3>
                        <div class="green" style="margin:0 -15px">安全运行天数</div>
                    </div>
                </div>
                <div class="col-lg-12 col-sm-12  b-b b-2x">
                    <h4 class="font-bold">园区电梯工单</h4>
                    <div style="height:200px;" ui-echarts="vm.pieData">
                    </div>
                </div>
                <div class="lift-panel col-lg-12 col-sm-12">
                    <h4 class="font-bold">{{vm.title}}电梯列表</h4>
                    <div class="content">
                        <div ng-repeat="item in vm.rightBuildEquipList" class="repeat-cont b-b clearfix">
                            <div class="col-sm-12">
                                <span class="keyName font-bold">电梯名称：</span>
                                <span class="keyValue">{{item.name}}</span>
                            </div>
                            <div class="col-sm-12">
                                <span class="keyName font-bold">电梯分类：</span>
                                <span class="keyValue">{{item.type_name}}</span>
                            </div>
                            <div class="col-sm-12">
                                <span class="keyName font-bold">设备编码：</span>
                                <span class="keyValue">{{item.equip_code}}</span>
                            </div>
                            <!-- <div class="col-sm-6">
                                <span class="keyName font-bold">运行状态：</span>
                                <span class="keyValue">上行</span>
                            </div>
                            <div class="col-sm-6">
                                <span class="keyName font-bold">电梯品牌：</span>
                                <span class="keyValue">西子奥的斯</span>
                            </div> -->
                            <div class="col-sm-12">
                                <span class="keyName font-bold">下次维保日期：</span>
                                <span class="keyValue">{{item.next_maintain_date}}</span>
                            </div>
                        </div>
                    </div>
            </div>
            </div>
            <!--展示单个楼栋的电梯情况-->
            <div ng-show="vm.showBuilding">
                <div class="col-lg-12 col-sm-12 b-b b-2x">
                    <h4 class="font-bold">{{vm.title}}电梯概况</h4>
                    <div class="col-lg-3 col-sm-3 text-center">
                        <h3>{{vm.rightBuildData.total}}</h3>
                        <div>电梯总数</div>
                    </div>
                    <div class="col-lg-3 col-sm-3 text-center">
                        <h3>{{vm.rightBuildData.regular_num}}</h3>
                        <div>正常</div>
                    </div>
                    <div class="col-lg-3 col-sm-3 text-center">
                        <h3>{{-(-vm.rightBuildData.broken_num - vm.rightBuildData.fail_num)}}</h3>
                        <div class="red">异常</div>
                    </div>
                    <div class="col-lg-3 col-sm-3 text-center  m-b-md">
                        <h3>{{vm.rightBuildData.max_regular_days}}</h3>
                        <div class="green">安全运行天数</div>
                    </div>
                </div>
                <div class="lift-panel col-lg-12 col-sm-12">
                    <h4 class="font-bold">{{vm.title}}电梯列表</h4>
                    <div class="content">
                        <div ng-repeat="item in vm.rightBuildEquipList" class="repeat-cont b-b clearfix">
                            <div class="col-sm-12">
                                <span class="keyName font-bold">电梯名称：</span>
                                <span class="keyValue">{{item.name}}</span>
                            </div>
                            <div class="col-sm-12">
                                <span class="keyName font-bold">电梯分类：</span>
                                <span class="keyValue">{{item.type_name}}</span>
                            </div>
                            <div class="col-sm-12">
                                <span class="keyName font-bold">设备编码：</span>
                                <span class="keyValue">{{item.equip_code}}</span>
                            </div>
                            <!-- <div class="col-sm-6">
                                <span class="keyName font-bold">运行状态：</span>
                                <span class="keyValue">上行</span>
                            </div>
                            <div class="col-sm-6">
                                <span class="keyName font-bold">电梯品牌：</span>
                                <span class="keyValue">西子奥的斯</span>
                            </div> -->
                            <div class="col-sm-12">
                                <span class="keyName font-bold">下次维保日期：</span>
                                <span class="keyValue">{{item.next_maintain_date}}</span>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </big-screen>
</div>
</div>
<style>
    .right-panel .lift-panel .content {
        height: 500px;
        overflow: auto;
    }
    
    .right-panel .lift-panel .content div {
        padding: 0;
    }
    
    .right-panel .lift-panel .content .repeat-cont {
        padding: 10px 0;
    }
    
    .right-panel .lift-panel .content .repeat-cont>div {
        margin: 5px 0;
    }
</style>