<div class="modal-header">
	<button type="button" class="close" ng-click="cancel()">×</button>
	<h4 class="modal-title">租赁空间</h4>
</div>
<div class="col-sm-6" style="margin-top:15px;">
	<label>待选区</label>
    <div class="well"
         style="padding-left: 0px; max-height: 600px; overflow: auto;">
        <tree-view node-list="treeData" config="config"></tree-view>
    </div>
</div>
<div class="col-sm-6" style="margin-top:15px;">
	<label>已选区</label>
	<div class="well">
		<ol style="padding-left: 15px">
			<li ng-repeat="item in rightList track by $index"
				ng-click="moveLeft(item)">{{item.fullPath}}</li>
		</ol>
	</div>
</div>
