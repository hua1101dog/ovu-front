<div class="modal-header">
    <h4 style="text-align: center">检测参数选择</h4>
</div>
<div class="modal-body">
    <table class="table">
        <thead>
            <tr>
                <th></th>
                <th>NO</th>
                <th>检测参数</th>
                <th>关联设备</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-if="!pageModel.data ||pageModel.data.length == 0"><td colspan="100" class="text-center">暂无记录</td></tr>
            <tr ng-repeat="item in pageModel.data">
                <td class="text-center" style="width:20px"><i role="button" ng-click="checkOne(item,pageModel.data)" ng-class="{'glyphicon-check':item.checked,'glyphicon-unchecked':!item.checked}" class="glyphicon"></i></td>
                <td>{{$index+1}}</td>
                <td>{{item.name}}</td>
                <td>{{item.equipName}}</td>
            </tr>
        </tbody>
    </table>
    <div class="page-footer clearfix" style="margin-left: 16px">
        <ng-include src="'/common/pager.html'"></ng-include>
    </div>
</div>
<div class="modal-footer">
    <button class="btn btn-primary" ng-click="save()">确定</button>
    <button class="btn btn-warning" ng-click="cancel()">取消</button>
</div>
<style>
    table tr th,table tr td {
        text-align: center;
    }
</style>